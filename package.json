{
    "name": "example_checker",
    "version": "1.0.0",
    "private": false,
    "description": "Action to cross reference examples in source code, README.md and Cargo.toml to ensure they contain the same info.",
    "main": "lib/Main.js",
    "scripts": {
        "build": "npx tsc",
        "format": "prettier --write **/*.ts",
        "format-check": "prettier --check **/*.ts",
        "lint": "eslint src/**/*.ts",
        "package": "npx ncc build src/Main.ts --source-map --license licenses.txt",
        "test": "jest",
        "all": "npm run format && npm run lint && npm run package",
        "dev": "nodemon --watch src -e ts,ejs --exec \"npm run build && npm run start\"",
        "start": "node ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Weibye/example_checker.git"
    },
    "bugs": {
        "url": "https://github.com/Weibye/example_checker/issues"
    },
    "homepage": "https://github.com/Weibye/example_checker#readme",
    "keywords": [
        "actions",
        "node",
        "setup"
    ],
    "author": "Andreas Weibye",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.2.6"
    },
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@types/node": "^14.14.9",
        "@typescript-eslint/parser": "^4.8.1",
        "@vercel/ncc": "^0.25.1",
        "eslint": "^7.17.0",
        "eslint-plugin-github": "^4.1.1",
        "eslint-plugin-jest": "^24.1.3",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "js-yaml": "^3.14.0",
        "nodemon": "^2.0.7",
        "prettier": "2.2.1",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    }
}
